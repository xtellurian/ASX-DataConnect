{
  "disabled": false,
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "asx-archive/basic/{timerTrigger}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "type": "eventHub",
      "name": "outputHub",
      "connection": "stockdata_RootManageSharedAccessKey_EVENTHUB",
      "eventHubName": "asx",
      "direction": "out"
    }
  ]
}
