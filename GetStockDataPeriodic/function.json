{
  "disabled": false,
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "asx-archive/basic/{timerTrigger}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
